{
  "name": "Diki słownik",
  "version": "0.0.2.7",
  "manifest_version": 3,
  "description":"Wielojęzyczny słownik Diki otwierany w podręcznym oknie.",
  "host_permissions" : [
    "http://*/*",
    "https://www.diki.pl/*"
  ],
  "permissions":["storage"],
  "icons":{
    "48": "diki_logo.png"
  },
  "action": {
    "default_icon":"diki_logo.png",
    "default_popup":"hello.html"
  }
}

